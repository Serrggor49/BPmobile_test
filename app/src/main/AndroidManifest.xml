<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="goryunov.bet.bpmobile_test">

    <application
        android:name="goryunov.bpmobile_test.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BPmobile_test">
        <activity
            android:name="goryunov.bpmobile_test.presentation.view.ProfileActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="goryunov.bpmobile_test.presentation.view.RegistrationActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="goryunov.bpmobile_test.presentation.view.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>